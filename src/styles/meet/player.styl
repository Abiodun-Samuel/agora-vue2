.player .user-vision .ban {
  z-index: 2;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  bottom: 0;
  padding: 2px 
  flex-direction: row;
  cursor: pointer;
  // border-radius: 5px;
  background-color: rgba(0, 0, 0, 0.4);
}
.player .user-vision .ban p {
  position: relative;
  margin: 2px 0;
  font-size: 8px;
  // padding: 5px;
  word-break: keep-all;
  white-space: nowrap;
  color: #ffffff
}

.player .user-vision .central {
  position: absolute;
  top: 0px;
  right: 0px;
  display: flex;
  align-items: center;
  justify-content: space-around;
  // transform: translate(-50%, -50%);
  padding: 5px;
  // background-color: rgba(210,210,210,0.8);
  border-radius: 10px;
  opacity: 1;
  transition: opacity 0.3s linear;
}
.player .user-vision:hover .central {
  opacity: 1;
  z-index: 2;
}
