.player {
  position: relative
  display: grid
  grid-gap: 5px
  grid-template-columns: repeat(auto-fit, minmax(150px, 1fr))
  min-height: auto
  // display: flex
  // flex-wrap: wrap
  // justify-content: center
  // align-items: stretch
  // width: 300px
  // height: 300px
}
.player>.user-vision {
  background-color: #333
  // flex: 0 0 33.33333%
}
.player .user-vision {
  position: relative
}
.player .user-vision .player-vision {
  width: 100%;
  height: 200px;
  // width: 100%;
  // height: 100%;
